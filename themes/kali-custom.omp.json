{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "git-clean": "#00ff00",
        "git-modified": "#ffaa00",
        "git-diverged": "#ff4444",
        "git-ahead": "#bb86fc",
        "git-behind": "#ff6b6b"
    },
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightBlue",
                    "properties": {
                        "display_host": true
                    },
                    "style": "plain",
                    "template": "<green>(</>{{ .UserName }}@{{ .HostName }}<green>)</>",
                    "type": "session"
                },
                {
                    "foreground": "lightWhite",
                    "properties": {
                        "folder_icon": "\uf115",
                        "folder_separator_icon": " \uf054 ",
                        "style": "agnoster_full"
                    },
                    "style": "plain",
                    "template": "<green>-[</>\udb81\udf70 {{ .Path }}<green>]</>",
                    "type": "path"
                },
                {
                    "foreground": "p:git-clean",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
                        "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
                        "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
                    ],
                    "style": "plain",
                    "template": "<green>-[</>{{ .UpstreamIcon }} {{.RepoName}} \uf061 {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <green>|</> <#ffaa00>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#00ff00>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <green>|</> <#87ceeb>\ueb4b {{ .StashCount }}</>{{ end }}<green>]</>",
                    "type": "git",
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "branch_icon": "\uf418 ",
                        "branch_identical_icon": "\uf444",
                        "branch_gone_icon": "\ueab8",
                        "fetch_upstream_icon": true
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "background": "#b2bec3",
                    "foreground": "#222222",
                    "leading_diamond": "\ue0b6",
                    "trailing_diamond": "<transparent,background>\ue0b2</>",
                    "properties": {
                        "linux": "\ue712",
                        "macos": "\ue711",
                        "windows": "\ue70f"
                    },
                    "style": "diamond",
                    "template": " {{.Icon}} ",
                    "type": "os"
                },
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b2",
                    "invert_powerline": true,
                    "foreground": "#88e570",
                    "background": "#193549",
                    "properties": {
                        "display_mode": "files",
                        "fetch_package_manager": true,
                        "fetch_version": true,
                        "npm_icon": "<#e5272d>\ue616 npm</> ",
                        "yarn_icon": "<#37aee5>\ue6a7 yarn</> ",
                        "pnpm_icon": "<#e5a100>\udb80\udec1 pnpm</> ",
                        "bun_icon": "<#e5a100>\ue76f bun</> "
                    },
                    "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}\ue718 {{ .Major }}.{{ .Minor }} "
                },
                {
                    "type": "python",
                    "style": "diamond",
                    "leading_diamond": "\ue0b2",
                    "trailing_diamond": "\ue0b4",
                    "foreground": "#ffd343",
                    "background": "#306998",
                    "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Major }}.{{ .Minor }} {{ if .Venv }}({{ .Venv }}) {{ end }}{{ end }}"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightBlue",
                    "style": "plain",
                    "template": "<green></><#FF96A0>\u276f</><#C591E8>\u276f</><#69FF94>\u276f</>",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "enable_cursor_positioning": true,
    "version": 3
}