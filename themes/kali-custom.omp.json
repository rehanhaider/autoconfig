{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightBlue",
                    "properties": {
                        "display_host": true
                    },
                    "style": "plain",
                    "template": "<green>(</>{{ .UserName }}@{{ .HostName }}<green>)</>",
                    "type": "session"
                },
                {
                    "foreground": "lightWhite",
                    "properties": {
                        "folder_separator_icon": "<#c0c0c0>/</>",
                        "style": "full"
                    },
                    "style": "plain",
                    "template": "<green>-[</>{{ .Path }}<green>]</>",
                    "type": "path"
                },
                {
                    "foreground": "white",
                    "foreground_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#a97400{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#54433a{{ end }}",
                        "{{ if gt .Ahead 0 }}#744d89{{ end }}",
                        "{{ if gt .Behind 0 }}#744d89{{ end }}"
                    ],
                    "style": "plain",
                    "template": "<green>-[</>{{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <#121318>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#121318>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <#121318>\ueb4b {{ .StashCount }}</>{{ end }}<green>]</>",
                    "type": "git",
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "branch_icon": "\uf418 ",
                        "branch_identical_icon": "\uf444",
                        "branch_gone_icon": "\ueab8",
                        "fetch_upstream_icon": true
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "type": "node",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#88e570",
                    "background": "#193549",
                    "properties": {
                        "display_mode": "files",
                        "fetch_package_manager": true,
                        "fetch_version": true,
                        "npm_icon": "<#e5272d>\ue616 npm</> ",
                        "yarn_icon": "<#37aee5>\ue6a7 yarn</> ",
                        "pnpm_icon": "<#e5a100>\udb80\udec1 pnpm</> "
                    },
                    "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}\ue718 {{ .Major }}.{{ .Minor }} "
                },
                {
                    "type": "python",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "foreground": "#ffd343",
                    "background": "#306998",
                    "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Major }}.{{ .Minor }} {{ if .Venv }}({{ .Venv }}) {{ end }}{{ end }}"
                },
                {
                    "background": "#565656",
                    "foreground": "#faa029",
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "template": " \ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}} ",
                    "type": "aws"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "lightBlue",
                    "style": "plain",
                    "template": "<green></><#FF96A0>\u276f</><#C591E8>\u276f</><#69FF94>\u276f</>",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "enable_cursor_positioning": true,
    "version": 3
}